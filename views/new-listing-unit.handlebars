{{> nav}}
<div class="container main-card mt-5">
  <div class="row d-flex justify-content-center unit-card">
    <div class="card col-md address-input m-2 p-0 inner-card">
      <div class="card-body container">
        <div class="row d-flex justify-content-center">
          <div class="col">
            {{#each buildings as |building|}}
            <h2 class="px-5">{{building.street_address}} New Unit</h2>
            {{/each}}
          </div>
        </div>
        <hr>
        {{> unit-form}}
        {{> full-unit-form}}
        {{> new-listing-unit-amenities}}
        {{> unit-images}}
      </div>
    </div>
  </div>
</div>
<script src='/js/new-listing.js'></script>
